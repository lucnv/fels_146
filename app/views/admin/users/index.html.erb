<% provide :title, t("views.admin.users.index.title") %>
<h1><%= t "views.admin.users.index.content" %></h1>
<%= will_paginate %>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user %>
      <%= link_to user.name, user %>
    </li>
  <% end %>
</ul>
<%= will_paginate %>
